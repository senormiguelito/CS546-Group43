{{#if unauthorizedAccess}}
<div class="Error">
  <h1>No, no, no!</h1>
  <h3>You are not authorized to view this content</h3>
  <h3> Please Login </h3>
  <a href="/">Want to Login? Click Here</a>
</div>
{{/if}}
{{#if badInput}}
<div class="Error">
  <p>
    {{error}}
  </p>
</div>
<div>
  <a href="/" >Want to Login? Click Here</a>
</div>
{{/if}}