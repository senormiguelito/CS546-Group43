{{#if unauthorizedAccess}}
<div class="Error">
  <h1>No, no, no!</h1>
  <h2>Session Expire, You are not authorized to view this content</h2>
  <h3> Please Login First</h3>
  <a class="link" href="/">Want to Login? Click Here</a>
</div>
{{/if}}
{{#if badInput}}
<div class="Error">
  <p>
    {{error}}
  </p>
</div>
<div>
  <a href="/" >Want to Login? Click Here</a>
</div>
{{/if}}
{{#if serverError}}
<h1>"Internal Server Error</h1>
<p>
  {{error}}
</p>
{{/if}}